{
  "manifest_version": 2,
  "name": "카카오 비즈니스 채팅창 크기 조절",
  "version": "1.0.0",
  "description": "카카오톡 비즈니스 채팅창의 크기를 자유롭게 조절할 수 있습니다.",
  "content_scripts": [
    {
      "matches": [
        "*://business.kakao.com/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "kakao-business-chat-resize@ehre.dev",
      "strict_min_version": "140.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}